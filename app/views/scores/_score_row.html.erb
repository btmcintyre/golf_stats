<tr>
	<td>
		<label>Hole <%=i%>:</label>
	</td>
	<td>
		<%= f.number_field ("score_" + i.to_s).to_sym, class: "form-control", placeholder: "Score" %>
	</td>
	<td>
		<%= f.number_field ("putts_" + i.to_s).to_sym, class: "form-control", placeholder: "Putts" %>
	</td>
	<td>
		<%= f.check_box ("fairways_" + i.to_s).to_sym, class: "form-control" , placeholder: "F"%>
	</td>
	<td>
		<%= f.check_box ("greens_" + i.to_s).to_sym, class: "form-control" , placeholder: "G"%>
	</td>

	<td>
	</td>

	<td>
		<label>Hole <%=(i+9)%>:</label>
	</td>
	<td>
		<%= f.number_field ("score_" + (i+9).to_s).to_sym, class: "form-control", placeholder: "Score" %>
	</td>
	<td>
		<%= f.number_field ("putts_" + (i+9).to_s).to_sym, class: "form-control", placeholder: "Putts" %>
	</td>
	<td>
		<%= f.check_box ("fairways_" + (i+9).to_s).to_sym, class: "form-control" , placeholder: "F"%>
	</td>
	<td>
		<%= f.check_box ("greens_" + (i+9).to_s).to_sym, class: "form-control" , placeholder: "G"%>
	</td>

</tr>
